
fn rec(n) {
    if (n == 10) {
        return 10
    }

    a = 1
    a = a + rec(n + 1)
    a = a - 1
    return a
}

print(rec(0))
print(rec(3))
print(rec(8))

fn fibonacci(n) {
    if(n <= 1) return 0
    if(n == 2) return 1

    a = fibonacci(n - 1)
    b = fibonacci(n - 2)

    return a + b
}

print(fibonacci(3))
print(fibonacci(8))
print(fibonacci(69))