
impl Enumerable for String {	

	fn GetIterator () {
		return Iterator(self)
	}

	fn Get (i) {
		return self[i]
	}

	fn IsInRange (i) {
		return i < self.Length
	}
} 

str = "hello world"

iterator = str.GetIterator()

print(iterator.Current)
print(iterator.Next())
