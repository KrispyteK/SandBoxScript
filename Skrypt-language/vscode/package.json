{
	"name": "skrypt-language",
	"displayName": "Skrypt Language Support",
	"description": "Snippits and syntax support for the Skrypt language by KrypteK",
	"version": "0.1.0",
	"publisher": "WYVERN",
	"author": {
		"name": "WYVERN"
	},
	"license": "MIT",
	"icon": "icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/KrispyteK/SkryptANTLR"
	},
	"homepage": "https://github.com/KrispyteK/SkryptANTLR/blob/master/Skrypt-language/vscode/README.md",
	"bugs": {
		"url": "https://github.com/KrispyteK/SkryptANTLR/issues"
	},
	"preview": true,
	"galleryBanner":{
		"color": "#A0AF00",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.30.1"
	},
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"contributes": {
		"languages": [
			{
				"id": "skrypt",
				"extensions": [
					".skt"
				],
				"aliases": [
					"Skrypt",
					"skrypt"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "skrypt",
				"scopeName": "source.skrypt",
				"path": "./syntaxes/skrypt.tmLanguage"
			}
		],
		"snippets": [
			{
				"language": "skrypt",
				"path": "./snippets.code-snippets"
			}
		]
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25"
	}
}
