{
	"name": "skrypt-language",
	"displayName": "Skrypt Language Support",
	"description": "Support for the Skrypt language by KrypteK",
	"version": "0.0.1",
	"author": "WYVERN",
	"engines": {
		"vscode": "^1.30.1"
	},
	"dependencies": {
		"vscode": "^1.1.26"
	},
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"contributes": {
		"languages": [
			{
				"id": "skrypt",
				"extensions": [
					".skt"
				],
				"aliases": [
					"Skrypt",
					"skrypt"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "skrypt",
				"scopeName": "source.skrypt",
				"path": "./syntaxes/skrypt.tmLanguage"
			}
		],
		"snippets": [
			{
				"language": "skrypt",
				"path": "./snippets.code-snippets"
			}
		]
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25"
	}
}
